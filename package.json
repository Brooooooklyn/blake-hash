{
  "name": "@napi-rs/blake-hash",
  "version": "1.3.4",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "blake",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@node-rs/deno-lint": "^1.20.4",
    "@node-rs/xxhash": "^1.7.3",
    "@swc-node/register": "^1.10.7",
    "@swc/core": "^1.6.13",
    "@types/node": "^22.0.0",
    "ava": "^6.1.3",
    "benny": "^3.7.1",
    "blake2": "^5.0.0",
    "blake3": "^3.0.0",
    "esbuild": "^0.25.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "workerThreads": false,
    "timeout": "2m",
    "require": [
      "@swc-node/register"
    ],
    "files": [
      "__test__/**/*.spec.ts"
    ]
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "lint": "denolint .",
    "prepublishOnly": "napi prepublish -t npm && esbuild --minify --outfile=index.js --allow-overwrite index.js",
    "version": "napi version",
    "test": "ava"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Brooooooklyn"
  },
  "repository": {
    "url": "git+ssh://git@github.com/Brooooooklyn/blake-hash.git",
    "type": "git"
  },
  "packageManager": "yarn@4.8.0",
  "resolutions": {
    "blake3-wasm@npm:2.1.7": "^3.0.0",
    "@c4312/blake3-native@npm:2.1.7": "^3.0.0",
    "@c4312/blake3-internal@npm:2.1.7": "^3.0.0"
  }
}
